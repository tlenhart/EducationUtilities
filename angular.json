{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "EducationUtilities": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "flat": false,
          "standalone": true
        },
        "@schematics/angular:service": {
          "flat": false
        },
        "@schematics/angular:pipe": {
          "flat": false,
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "eu",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/education-utilities",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "node_modules/clippy.modern/dist/agents",
                "output": "agents"
              },
              {
                "glob": "**/*",
                "input": "node_modules/clippy.modern/dist/agents-no-sound",
                "output": "agents-no-sound"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/loader.scss",
              "src/styles/themes/default/light-theme.scss",
              "src/styles/themes/wave/light-theme.scss",
              "src/styles/themes/pink-wave/light-theme.scss",
              {
                "bundleName": "clippy",
                "inject": true,
                "input": "node_modules/clippy.modern/dist/clippy.css"
              }
            ],
            "scripts": [
            ],
            "webWorkerTsConfig": "tsconfig.worker.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all",
              "baseHref": "/",
              "extractLicenses": true,
              "subresourceIntegrity": true,
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "EducationUtilities:build:production"
            },
            "development": {
              "buildTarget": "EducationUtilities:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "node_modules/clippy.modern/dist/agents",
                "output": "agents"
              },
              {
                "glob": "**/*",
                "input": "node_modules/clippy.modern/dist/agents-no-sound",
                "output": "agents-no-sound"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/loader.scss",
              "src/styles/themes/default/light-theme.scss",
              "src/styles/themes/wave/light-theme.scss",
              "src/styles/themes/pink-wave/light-theme.scss",
              {
                "bundleName": "clippy",
                "inject": true,
                "input": "node_modules/clippy.modern/dist/clippy.css"
              }
            ],
            "scripts": [],
            "webWorkerTsConfig": "tsconfig.worker.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
